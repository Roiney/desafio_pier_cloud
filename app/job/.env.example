PORT=3000

# Configurações do PostgreSQL para o banco de dados Store
PG_HOST_STORE="127.0.0.1"
PG_USER_STORE="postgres"
PG_PASSWORD_STORE="postgres"
PG_DATABASE_STORE="wavelight-Cloud"
