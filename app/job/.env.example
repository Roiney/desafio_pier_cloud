DATABASE_URL_GATEWAY="postgresql://${PG_USER_GATEWAY}:${PG_PASSWORD_GATEWAY}@${PG_HOST_GATEWAY}:${PG_PORT_GATEWAY}/${PG_DATABASE_GATEWAY}"



